// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema
datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

{{#each entities}}
model {{id}} {
  id        String  @id @default(uuid())
  {{#each fields}}
  {{label}}    {{type}}{{#if required}}{{else}}?{{/if}}  {{#if isUnique}}@unique{{/if}} {{#if default}}@default({{{default}}}){{/if}}
  {{/each}}
  {{#each parents}}
  {{id}}        {{id}}     @relation(fields: [{{id}}Id], references: [id])
  {{id}}Id      String
  {{/each}}
  {{#each children}}
  {{plural}}    {{id}}   []
  {{/each}}
}

{{/each}}